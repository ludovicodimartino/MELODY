###  http://www.dei.unipd.it/~gdb/ontology/melody#acousticness
mel:acousticness rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf owl:topDataProperty ;
                 rdf:type owl:FunctionalProperty ;
                 rdfs:domain mel:SongHot200 ;
                 rdfs:range xsd:float ;
                 rdfs:comment "A confidence measure from 0.0 to 1.0 of whether the track is acoustic. 1.0 represents high confidence the track is acoustic."@en ;
                 rdfs:label "acousticness"@en .


###  http://www.dei.unipd.it/~gdb/ontology/melody#artistType
mel:artistType rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdf:type owl:FunctionalProperty ;
               rdfs:domain mel:Artist ;
               rdfs:range [ rdf:type rdfs:Datatype ;
                            owl:oneOf [ rdf:type rdf:List ;
                                        rdf:first "DJ" ;
                                        rdf:rest [ rdf:type rdf:List ;
                                                   rdf:first "band" ;
                                                   rdf:rest [ rdf:type rdf:List ;
                                                              rdf:first "duo" ;
                                                              rdf:rest [ rdf:type rdf:List ;
                                                                         rdf:first "rapper" ;
                                                                         rdf:rest [ rdf:type rdf:List ;
                                                                                    rdf:first "singer" ;
                                                                                    rdf:rest rdf:nil
                                                                                  ]
                                                                       ]
                                                            ]
                                                 ]
                                      ]
                          ] ;
               rdfs:comment "Whether the artist is a DJ  or a band or a duo or a rapper or a singer."@en ;
               rdfs:label "artistType"@en .


###  http://www.dei.unipd.it/~gdb/ontology/melody#bpm
mel:bpm rdf:type owl:DatatypeProperty ;
        rdfs:subPropertyOf owl:topDataProperty ;
        rdf:type owl:FunctionalProperty ;
        rdfs:domain mel:SongHot200 ;
        rdfs:range xsd:positiveInteger ;
        rdfs:comment "The overall estimated tempo of a track in beats per minute."@en ;
        rdfs:label "bpm"@en .


###  http://www.dei.unipd.it/~gdb/ontology/melody#danceability
mel:danceability rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf owl:topDataProperty ;
                 rdf:type owl:FunctionalProperty ;
                 rdfs:domain mel:SongHot200 ;
                 rdfs:range xsd:float ;
                 rdfs:comment """Danceability describes how suitable a track is for dancing based on a combination of musical elements including tempo, rhythm stability, beat strength, and overall regularity. 
A value of 0.0 is least danceable and 1.0 is most danceable."""@en ;
                 rdfs:label "danceability"@en .


###  http://www.dei.unipd.it/~gdb/ontology/melody#date
mel:date rdf:type owl:DatatypeProperty ;
