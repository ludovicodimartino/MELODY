<http://www.dei.unipd.it/~gdb/ontology/melody> rdf:type owl:Ontology ;
                                                owl:imports <http://www.w3.org/TR/skos-reference/skos-owl1-dl.rdf> ;
                                                rdfs:comment "The goal of the project is to create a graph database containing information about the most popular weekly songs from 1958 to the present. This data will be gathered from various datasets and Spotify. With this information, it will be possible to make evaluations, for example, on song duration, key used, etc."@en ;
                                                rdfs:isDefinedBy """Di Martino Ludovico, 2104292
Galli Filippo, 2120826
Rigobello Manuel, 2103374"""@en .

#################################################################
#    Object Properties
#################################################################

###  http://www.dei.unipd.it/~gdb/ontology/melody#candidated
mel:candidated rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( mel:Album
                                           mel:Artist
                                           mel:SongHot200
                                         )
                           ] ;
               rdfs:range mel:Grammy ;
               rdfs:comment "This property describes the Album, the Artists and the Songs candidated  for a Grammy."@en ;
               rdfs:label "candidated"@en .


###  http://www.dei.unipd.it/~gdb/ontology/melody#classified
mel:classified rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               rdfs:domain mel:SongHot200 ;
               rdfs:range mel:Membership ;
               rdfs:comment "This property links a song to the Membership class that specifies the position in the Billboard 200."@en ;
               rdfs:label "classified"@en .


###  http://www.dei.unipd.it/~gdb/ontology/melody#classifiedIn
mel:classifiedIn rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdfs:domain mel:Membership ;
                 rdfs:range mel:BillboardHot100 ;
                 rdfs:comment "This property links the position of a song to the Billboard of a certain week."@en ;
                 rdfs:label "classifiedIn"@en .


###  http://www.dei.unipd.it/~gdb/ontology/melody#containsSong
mel:containsSong rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 owl:inverseOf mel:isInAlbum ;
                 rdfs:domain mel:Album ;
                 rdfs:range mel:SongHot200 ;
                 rdfs:comment "This property describes an album that contains a song."@en ;
                 rdfs:label "containsSong"@en .


###  http://www.dei.unipd.it/~gdb/ontology/melody#hasCategory
mel:hasCategory rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                rdfs:domain mel:Grammy ;
                rdfs:range mel:GrammyCategory ;
                rdfs:comment "This property describes the category of a Grammy."@en ;
                rdfs:label "hasCategory"@en .


###  http://www.dei.unipd.it/~gdb/ontology/melody#hasGenre
mel:hasGenre rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:domain mel:Artist ;
             rdfs:range mel:Genre ;
             rdfs:comment "This property links an artist to its genre."@en ;
             rdfs:label "hasGenre"@en .


###  http://www.dei.unipd.it/~gdb/ontology/melody#hasPopularity
mel:hasPopularity rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf owl:topObjectProperty ;
                  rdfs:domain mel:Artist ;
                  rdfs:range mel:Popularity ;
                  rdfs:comment "This property links an artist to its popularity (low, medium or high)."@en ;
                  rdfs:label "hasPopularity"@en .


###  http://www.dei.unipd.it/~gdb/ontology/melody#isInAlbum
mel:isInAlbum rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdfs:comment "This property links a song to its album."@en ;
              rdfs:label "isInAlbum"@en .


###  http://www.dei.unipd.it/~gdb/ontology/melody#isLessPopularThan
mel:isLessPopularThan rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf owl:topObjectProperty ;
                      owl:inverseOf mel:isMorePopularThan ;
                      rdf:type owl:TransitiveProperty ;
                      rdfs:domain mel:Popularity ;
                      rdfs:range mel:Popularity ;
                      rdfs:comment "This property states whether a certain polpolarity (low, medium or high) is less than another popularity."@en ;
                      rdfs:label "isLessPopularThan"@en .


###  http://www.dei.unipd.it/~gdb/ontology/melody#isMorePopularThan
mel:isMorePopularThan rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf owl:topObjectProperty ;
                      rdf:type owl:TransitiveProperty ;
                      rdfs:domain mel:Popularity ;
                      rdfs:range mel:Popularity ;
                      rdfs:comment "This property states whether a certain polpolarity (low, medium or high) is more than another popularity."@en ;
                      rdfs:label "isMorePopularThan"@en .


###  http://www.dei.unipd.it/~gdb/ontology/melody#isRemixOf
mel:isRemixOf rdf:type owl:ObjectProperty ,
                       owl:TransitiveProperty ;
              rdfs:domain mel:SongHot200 ;
              rdfs:range mel:SongHot200 ;
              rdfs:comment "This property links a song to its remix."@en ;
              rdfs:label "isRemixOf"@en .


###  http://www.dei.unipd.it/~gdb/ontology/melody#sing
mel:sing rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf owl:topObjectProperty ;
         owl:inverseOf mel:sungBy ;
         rdfs:domain mel:Artist ;
         rdfs:range mel:SongHot200 ;
         rdfs:comment "This property links an artist to a song that he/she sang."@en ;
         rdfs:label "sing"@en .


###  http://www.dei.unipd.it/~gdb/ontology/melody#sungBy
mel:sungBy rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdfs:label "This property links a song to an artist that sings that song."@en ,
                      "sungBy"@en .


###  http://www.dei.unipd.it/~gdb/ontology/melody#winner
mel:winner rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf mel:candidated ;
           rdfs:domain [ rdf:type owl:Class ;
                         owl:unionOf ( mel:Album
                                       mel:Artist
                                       mel:SongHot200
                                     )
                       ] ;
           rdfs:range mel:Grammy ;
           rdfs:comment "This property describes the Albums, the Artists and the Songs that won a Grammy."@en ;
           rdfs:label "winner"@en .


