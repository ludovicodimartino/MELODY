#################################################################
#    Classes
#################################################################

###  http://www.dei.unipd.it/~gdb/ontology/melody#Album
mel:Album rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Restriction ;
                                owl:onProperty mel:containsSong ;
                                owl:someValuesFrom mel:SongHot200
                              ] ;
          rdfs:comment "The class representing the albums."@en ;
          rdfs:label "Album"@en .


###  http://www.dei.unipd.it/~gdb/ontology/melody#Artist
mel:Artist rdf:type owl:Class ;
           owl:equivalentClass [ rdf:type owl:Restriction ;
                                 owl:onProperty mel:sing ;
                                 owl:someValuesFrom mel:SongHot200
                               ] ;
           rdfs:comment "The class representing the artists."@en ;
           rdfs:label "Artist"@en .


###  http://www.dei.unipd.it/~gdb/ontology/melody#BillboardHot100
mel:BillboardHot100 rdf:type owl:Class ;
                    rdfs:comment "The class representing the Billboard Hot 200."@en ;
                    rdfs:label "BillboardHot200"@en .


###  http://www.dei.unipd.it/~gdb/ontology/melody#Genre
mel:Genre rdf:type owl:Class ;
          rdfs:comment "The class representing the genre."@en ;
          rdfs:label "Genre"@en .


###  http://www.dei.unipd.it/~gdb/ontology/melody#Grammy
mel:Grammy rdf:type owl:Class ;
           rdfs:comment "The class representing the Grammy Award."@en ;
           rdfs:label "Grammy"@en .


###  http://www.dei.unipd.it/~gdb/ontology/melody#GrammyCategory
mel:GrammyCategory rdf:type owl:Class ;
                   rdfs:comment "The class representing the Grammy Award category."@en ;
                   rdfs:label "GrammyCategory"@en .


###  http://www.dei.unipd.it/~gdb/ontology/melody#Membership
mel:Membership rdf:type owl:Class ;
               rdfs:comment "The class representing the Membership between the song and its position in the Billboard Hot 200 in the specified date."@en ;
               rdfs:label "Membership"@en .


###  http://www.dei.unipd.it/~gdb/ontology/melody#Popularity
mel:Popularity rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Class ;
                                     owl:oneOf ( mel:High
                                                 mel:Low
                                                 mel:Medium
                                               )
                                   ] ;
               rdfs:comment "The class representing the popularity."@en ;
               rdfs:label "Popularity"@en .


###  http://www.dei.unipd.it/~gdb/ontology/melody#SongHot200
mel:SongHot200 rdf:type owl:Class ;
               owl:equivalentClass [ rdf:type owl:Restriction ;
                                     owl:onProperty mel:sungBy ;
                                     owl:someValuesFrom mel:Artist
                                   ] ;
               rdfs:comment "The class representing the song in the Billboard Hot 200."@en ;
               rdfs:label "SongHot200"@en .


#################################################################
#    Individuals
#################################################################

###  http://www.dei.unipd.it/~gdb/ontology/melody#High
mel:High rdf:type owl:NamedIndividual ,
                  mel:Popularity ;
         mel:isMorePopularThan mel:Medium .


###  http://www.dei.unipd.it/~gdb/ontology/melody#Low
mel:Low rdf:type owl:NamedIndividual ,
                 mel:Popularity ;
        mel:isLessPopularThan mel:Medium .


###  http://www.dei.unipd.it/~gdb/ontology/melody#Medium
mel:Medium rdf:type owl:NamedIndividual ,
                    mel:Popularity ;
           mel:isLessPopularThan mel:High ;
           mel:isMorePopularThan mel:Low .


